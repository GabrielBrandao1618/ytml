text       = @{ (ASCII_ALPHANUMERIC | "-")* }
string     = ${ "\"" ~ text ~ "\"" }
tag_name   = { text }
prop_name  = { text }
prop_value = { string }
tag_prop   = { prop_name ~ "=" ~ prop_value }
tag        = { tag_name ~ "(" ~ tag_prop* ~ ")" ~ "{" ~ tag* | text ~ "}" }

doc = { SOI ~ tag* ~ EOI }

WHITESPACE = _{ " " | "\n" }
